{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "proxyHomePage": {
            "matchCondition": {
                "methods": [
                    "GET",
                    "OPTIONS"
                ],
                "route": "/web/"
            },
            "backendUri": "https://knowshow.z33.web.core.windows.net/index.html"
        },
        "proxyCss": {
            "matchCondition": {
                "methods": [
                    "GET",
                    "OPTIONS"
                ],
                "route": "/css/{*restOfPath}"
            },
            "backendUri": "https://knowshow.z33.web.core.windows.net/css/{restOfPath}"
        },
        "proxyComponents": {
            "matchCondition": {
                "methods": [
                    "GET",
                    "OPTIONS"
                ],
                "route": "/components/{*restOfPath}"
            },
            "backendUri": "https://knowshow.z33.web.core.windows.net/components/{restOfPath}"
        }
    }
}